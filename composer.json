{
    "name": "silivanov/scout-elasticsearch-driver",
    "type": "library",
    "description": "The Elasticsearch Driver for Laravel Scout",
    "keywords": [
        "elastic",
        "elasticsearch",
        "driver",
        "engine",
        "laravel",
        "scout",
        "search"
    ],
    "homepage": "https://github.com/andrey-silivanov/scout-elasticsearch-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Silivanov Andrei",
            "email": "andrey.silivanovv@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.|^8.0",
        "elasticsearch/elasticsearch": "8.*",
        "laravel/scout": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ScoutElastic\\Tests\\": "tests/",
            "ScoutElastic\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ScoutElastic\\ScoutElasticServiceProvider"
            ]
        }
    }
}
